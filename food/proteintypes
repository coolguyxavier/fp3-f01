# - Path \\Code \Food \proteintypes

# - Imports - #
from random import choice

# - Dictionaries - #

proteins = {"Pork": 'Thigh', "Chicken": 'Drumstick', "Goat": 'Brain'}

# - Functions - #

def findprotein():
    randfood = choice(list(proteins))
    return randfood
    
def findtype(food):
    foodtype = proteins[food]
    return foodtype

